<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">

    <record id="l10n_do_citizen_validation_oauth_url" model="ir.config_parameter">
        <field name="key">citizen.oauth.url</field>
        <field name="value">https://api.am.digital.gob.do/digital/oauth/token</field>
    </record>
    <record id="l10n_do_citizen_validation_api_url" model="ir.config_parameter">
        <field name="key">citizen.api.url</field>
        <field name="value">https://api.digital.gob.do/v3/citizens</field>
    </record>
    <record id="l10n_do_citizen_validation_api_token" model="ir.config_parameter">
        <field name="key">citizen.api.token</field>
        <field name="value"></field>
    </record>

</odoo>
